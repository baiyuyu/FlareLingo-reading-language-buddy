{
  "manifest_version": 3,
  "name": "FlareLingo â€“ Reading Language Buddy",
  "version": "1.0.5",
  "description": "FlareLingo: subtle inline language stimulus while you read.",
  "permissions": ["storage", "activeTab", "scripting", "aiLanguageModel"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    }
  },
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/ai/ai-core.js", "js/content.js"],
      "css": ["css/highlighter.css"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["data/dutch_frequency_dictionary.json"],
      "matches": ["<all_urls>"]
    }
  ],
